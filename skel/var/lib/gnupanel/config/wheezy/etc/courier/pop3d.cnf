
RANDFILE = /usr/lib/courier/pop3d.rand

[ req ]
default_bits = 1024
encrypt_key = yes
distinguished_name = req_dn
x509_extensions = cert_type
prompt = no

[ req_dn ]
C=AR
ST=Buenos Aires
L=Capital Federal
O=Courier Mail Server
OU=Automatically-generated POP3 SSL key
CN=gnupanel.org
emailAddress=rmalvarez@gnupanel.org


[ cert_type ]
nsCertType = server
